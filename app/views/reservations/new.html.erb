<h1>New Reservation</h1>
<%= form_for :reservation, url: reservations_path do |f| %>
 <p>
    <%= f.label :lot %><br>
    <%= f.text_area :lot %>
  </p>
 
  <p>
    <%= f.label :license %><br>
    <%= f.text_field :license %>
  </p>
  
  <p>
    <%= f.label :make %><br>
    <%= f.text_field :make %>
  </p>
  
  <p>
    <%= f.label :model %><br>
    <%= f.text_field :model %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>
